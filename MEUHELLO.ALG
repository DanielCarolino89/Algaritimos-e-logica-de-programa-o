Algoritmo "MeuHello"

Var
// Seção de Declarações das variáveis 
 Nome, Qualidade : caracter

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
 escreval("Me diga seu nome")
 leia(Nome)
 
  ESCREVAL("Me diga uma qualidade sua")
  leia(Qualidade)
  
  escreval("Ola, ", Nome, " voce e muito ", qualidade)


Fimalgoritmo